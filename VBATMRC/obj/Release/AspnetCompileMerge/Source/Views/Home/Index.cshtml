            padding: 0;
            top: 15px;
            position: relative;
            height: 7.5em;
            padding: 0;
        }

        /**, *:before, *:after {
            -webkit-box-sizing: content-box !important;
            -moz-box-sizing: content-box !important;
            box-sizing: content-box !important;
        }*/

        .featured .content-wrapper {
            background-color: #ffffff;
            background-image: none;
            padding: 30px 30px 30px 30px;
        }

        .main-content {
            background-image: none !important;
            padding: 0;
            max-width: 1000px;
        }

        .main-panel {
            min-width: 400px;
            max-width: 962px;
            margin: 0 auto !important;
            background: white;
        }

        .nav-custom, .nav-btm-custom {
            font-size: 0.85em;
            line-height: 1.1em;
            font-weight: bold;
        }

            .nav-custom .last > a:link, .nav-custom .last > a:visited, .nav-custom .last > a:active {
                border-top-right-radius: 12px !important;
                border-bottom-right-radius: 0;
                border-top-left-radius: 0;
                border-bottom-left-radius: 0;
                border-right-style: none;
            }

            .nav-custom .first > a:link, .nav-custom .first > a:visited, .nav-custom .first > a:active {
                border-top-right-radius: 0;
                border-bottom-right-radius: 0;
                border-top-left-radius: 12px !important;
                border-bottom-left-radius: 0;
            }

            .nav-btm-custom .last > a:link, .nav-btm-custom .last > a:visited, .nav-btm-custom .last > a:active {
                border-top-right-radius: 0;
                border-bottom-right-radius: 12px !important;
                border-top-left-radius: 0;
                border-bottom-left-radius: 0;
                border-right-style: none;
            }

            .nav-btm-custom .first > a:link, .nav-btm-custom .first > a:visited, .nav-btm-custom .first > a:active {
                border-top-right-radius: 0;
                border-bottom-right-radius: 0;
                border-top-left-radius: 0;
                border-bottom-left-radius: 12px !important;
            }


            .nav-custom > li > a:link, .nav-custom > li > a:visited,
            .nav-custom > li > a:active {
                background-color: lightsteelblue;
                color: black !important;
                text-decoration: none;
                height: 43px;
                /*height: 18px;*/
                padding-bottom: 14px;
                border-right-style: groove;
                border-right-color: gainsboro;
                border-right-width: .2em;
                border-top-right-radius: 0;
                border-bottom-right-radius: 0;
                border-top-left-radius: 0;
                border-bottom-left-radius: 0;
            }

            .nav-btm-custom > li > a:link, .nav-btm-custom > li > a:visited,
            .nav-btm-custom > li > a:active {
                color: white !important;
                text-decoration: none;
                height: 43px;
                /*height: 18px;*/
                padding-bottom: 14px;
                border-right-style: groove;
                border-right-color: dodgerblue;
                border-right-width: .2em;
                border-top-right-radius: 0;
                border-bottom-right-radius: 0;
                border-top-left-radius: 0;
                border-bottom-left-radius: 0;
            }


            .nav-custom > li > a:link, .nav-custom > li > a:visited {
                background-color: rgb(178, 193, 226);
            }

            .nav-custom > li > a:active, .nav-custom > li > a:hover {
                background-color: rgb(130, 140, 179);
                color: white !important;
            }

            .nav-btm-custom > li > a:link, .nav-btm-custom > li > a:visited {
                background-color: #0177c0;
            }

            .nav-btm-custom > li > a:active, .nav-btm-custom > li > a:hover {
                background-color: rgb(132, 186, 253);
                color: black !important;
            }

        .well-sm {
            border-radius: 16px;
        }

        .input-sm {
            padding: 5px 0;
        }
    </style>

}
@section scripts {
    <script type="text/javascript">
        $(function () {

            $(".thumbnail").tooltip({
                delay: {
                    show: 10,
                    hide: 0
                }
            });

            $(".nav-custom a").tooltip();
            $(".nav-btm-custom a").tooltip({
                placement: "bottom"
            });
            //$(".thumbnail").css({ "border-style": "solid", "background-color": "#7cc0d9" });


            $('a.thumbnaildisabled').click(function (event) {
                event.preventDefault(); // Prevent link from following its href
            })/*.css("cursor", "not-allowed")*/;

        });

    </script>

}


                        <li class="list-group-item">
                            <a href="~/Assets/Documents/2.2 TM Standard Operating Procedures.pdf" target="_blank">TM Standard Operating Procedures</a>
                        </li>
                    </ul>
                </section>
            </div>
        </div>
    </div>
</div>
@section scripts
    {
    <script type="text/javascript">
        var baseUrl = "@Url.Content("~")";
        baseUrl = baseUrl == "/" ? "" : baseUrl;
        baseUrl += "/Home/ReadXMLFromFile/";
    </script>
}

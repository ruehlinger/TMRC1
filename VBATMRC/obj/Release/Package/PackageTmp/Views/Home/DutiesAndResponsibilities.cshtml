@using VBATMRC.Models
@{
    ViewBag.Title = "TM Duties & Responsibilities";
}

@section styles
{
    <style type="text/css">
        #drlist {
            list-style: none;
        }
    </style>

}
<div class="content-wrapper panel main-panel">
    <!-- ANNOUNCEMENTS BALOON HERE -->
    @{
        if (ViewBag.Dev)
        {
            Html.RenderPartial("_Announcements", new Announcement());
        }
        Html.RenderPartial("_PageHeader");
    }
    <div class="content-wrapper">
        <div class="presentationPanel01">
            <div class="panel-body">
               <h6 style="float: right; color: #808080;">Last Updated: 4/14/15</h6>
                <p>&nbsp;</p>
                <section>
                    
                    <ul class="list-group">
                        <li class="list-group-item">
                            <a href="~/Assets/Documents/2.1 Program Specialist Training Manage_GS12-FY08.pdf" target="_blank">GS-12 Position Description for TMs (2015)</a>
